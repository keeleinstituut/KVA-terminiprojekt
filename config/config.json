{
    "embeddings": {
        "embedding_model": "intfloat/multilingual-e5-large",
        "embedding_size": 1024,
        "max_tokens": 250,
        "passage_prompt": "passage: ",
        "query_prompt": "query: "
    },
    "hybrid_search": {
        "enabled": true,
        "sparse_model": "Qdrant/bm25",
        "dense_weight": 0.7,
        "sparse_weight": 0.3
    },
    "reranking": {
        "enabled": false,
        "model": "BAAI/bge-reranker-v2-m3",
        "top_k": 5
    },
    "llm": {
        "model": "gpt-4o",
        "temperature": 0
    },
    "intermediate_storage": {
        "json_storage_path": "../data/temp/",
        "finished_json_storage_path": "../data/finished/"
    }
}